<div class='container'>
<div class="row search-result">
  <% @users.each do |user| %>
    <div class="col-md-10 col-xs-offset-1 well member">
      <%= user.full_name %>
      <%= link_to "Follow", friendships_path(friend_id: user.id), method: :post, class: "btn btn-primary pull-right" unless current_user.follows_or_same?(user) %>
    </div>
  <% end %>
</div>
<%= will_paginate @users, renderer: BootstrapPagination::Rails, class: "pull-left paginate" %>
</div>
