<div class="row">
  <div class="col-md-10">
    <h2>List of Members</h2>
    <%= will_paginate @users, renderer: BootstrapPagination::Rails, class: "pull-left paginate" %>
  </div>
</div>

<div class="row">
  <% @users.each do |u| %>
    <div class="col-md-10 well member">
      <%= u.full_name %>
    </div>
  <% end %>
</div>
